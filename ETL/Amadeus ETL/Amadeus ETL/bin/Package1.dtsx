re s&#xA;  on oh.store_number = s.store_number&#xA;where oh2.sales_date &lt;&gt; oh.order_date&#xA;or oh2.customer_key &lt;&gt; coalesce(c.customer_key, 0)&#xA;or oh2.store_key &lt;&gt; coalesce(s.store_key, 0)&#xA;or oh2.source_system_code &lt;&gt; 1 &#xA;" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable><DTS:Property DTS:Name="ObjectName">Package1</DTS:Property><DTS:Property DTS:Name="DTSID">{33944366-DCD1-4DB0-BA36-2D150C635211}</DTS:Property><DTS:Property DTS:Name="Description"></DTS:Property><DTS:Property DTS:Name="CreationName">MSDTS.Package.1</DTS:Property><DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property></DTS:Executable>